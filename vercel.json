{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/users/(.*)",
        "dest": "app.js"
      },
      {
        "src": "/profile/(.*)",
        "dest": "app.js"
      },
      {
        "src": "/person/(.*)",
        "dest": "app.js"
      },
      {
        "src": "/",
        "dest": "app.js"
      }
    ]
  }